{
  "name": "vt-api",
  "version": "0.6.0",
  "description": "Simple API for getting VTuber livefeeds, channels, and videos.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index",
    "save-channels": "node -e \"require('./channels').save()\"",
    "scrape-channels": "node -e \"require('dotenv').config() && require('./apps/api').init()\"",
    "init": "node -e \"require('./channels').init()\""
  },
  "keywords": [],
  "author": "",
  "license": "WTFPL",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "memcached": "^2.2.2",
    "memcached-promise": "^1.0.1",
    "mongoist": "^2.5.3",
    "node-fetch": "^2.6.1",
    "node-schedule": "^1.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.48",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@types/node-schedule": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.3"
  },
  "nodemonConfig": {
    "ignore": [
      "channels/"
    ],
    "events": {
      "start": "clear",
      "restart": "clear"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Choooks22/vt-api.git"
  },
  "bugs": {
    "url": "https://github.com/Choooks22/vt-api/issues"
  },
  "homepage": "https://github.com/Choooks22/vt-api#readme"
}
